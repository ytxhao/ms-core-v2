print("======== 12 android:$is_android")
if (is_android) {
  print("======== 13 android:$is_android")
  import("//build/config/android/config.gni")
  print("======== 14 android:$is_android")
  import("//build/config/android/rules.gni")
  print("======== 15 android:$is_android")
  import("../../ms-core.gni")
  print("======== 16 android:$is_android")

  group("android") {
    if (is_android) {
      public_deps = [
        ":libms-framework",
      ]
    }
  }
  dist_jar("libms-framework") {
    _target_dir_name = get_label_info(":$target_name", "dir")
    output = "${root_out_dir}/lib.java${_target_dir_name}/${target_name}.jar"
    direct_deps_only = true
    use_unprocessed_jars = true
    requires_android = true
    no_build_hooks = true

    deps = [
      ":ms_java"
    ]
  }

  rtc_android_library("ms_java") {
    sources = [
      "api/media/ushow/ms/Constants.java",
    ]
    deps = []
  }
}
